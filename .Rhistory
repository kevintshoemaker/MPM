eqfun = eq, eqB = c(0,0,0),
ineqfun = ineq, ineqLB = rep(-1e9,length(aam)-2), ineqUB=rep(0,length(aam)-2),
LB=bounds[,1],UB=bounds[,2])   #
sink()
data.frame(
ages=aam,
prob=s$pars
)
}
